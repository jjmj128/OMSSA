        <rdfs:label xml:lang="en">Cost measurment ICE</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000030ROMAIN -->

    <owl:Class rdf:about="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000030ROMAIN">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000029ROMAIN"/>
        <rdfs:label xml:lang="en">Actual Cost</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000031ROMAIN -->

    <owl:Class rdf:about="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000031ROMAIN">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000029ROMAIN"/>
        <rdfs:label xml:lang="en">Planned Cost</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000032ROMAIN -->

    <owl:Class rdf:about="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000032ROMAIN">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/MeasurementInformationContentEntity"/>
        <rdfs:label xml:lang="en">Time measurement ICE</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <CommonCoreOntologies:definition>A BFO: quality sub-class that a COMSA: physical equipment bears. It qualifies the operation state of the physical equipment. It states the operational parameters of the physical equipment described by COMSA: condition data and prescribed by COMSA: condition specification.</CommonCoreOntologies:definition>
        <rdfs:label>Condition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_data -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_data">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DescriptiveInformationContentEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/describes"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_input_of"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_input_of"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Fault_detection_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_input_of"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Prognostic_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_output_of"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_monitoring"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A CCO: Descriptive Information Content Entity that consists of a set of propositions that describes COMSA: Condition</CommonCoreOntologies:definition>
        <rdfs:label>Condition data</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_monitoring -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_monitoring">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/has_output"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_data"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A BFO: process that has as output COMSA: Condition data</CommonCoreOntologies:definition>
        <rdfs:label>Condition monitoring</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_specification -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition_specification">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/QualitySpecification"/>
        <CommonCoreOntologies:definition>A CCO: Quality specification sub-class that prescribes the MSO: condition</CommonCoreOntologies:definition>
        <rdfs:label>Condition specification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Corrective_maintenance -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Corrective_maintenance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Maintenance_Strategy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_about"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A COMSA: maintenance strategy whose triggering event is the failure occurrence</CommonCoreOntologies:definition>
        <rdfs:label>Corrective maintenance</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Cost-Risk-Benefit_Analysis -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Cost-Risk-Benefit_Analysis">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/has_output"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#cost-benefit-risk_analysis_report"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/process_preceded_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_Modes,_Effects_and_Criticality_Analysis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A BFO: process which iw part of the strategy development process, it aims at evaluating the implementation of a more advanced maintenance strategy for a physical equipment compared to the current one. It considers the failure risk, the cost and benefits of its implementation.</CommonCoreOntologies:definition>
        <rdfs:label>Cost-Risk-Benefit Analysis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DecreaseOfFunction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/affects"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/process_preceded_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Nominal_operation_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A CCO: Decrease of function. It represents a detrimental change in the operating condition of a physical equipment, normally as a result of a fault.</CommonCoreOntologies:definition>
        <rdfs:label>Degradation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_module -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_module">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Predictive_maintenance_system_module"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000056"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000116>Can also be called health assessment module</obo:IAO_0000116>
        <CommonCoreOntologies:definition>A COMSA: predictive maintenance system subclass which aims at determining the degradation level in a physical system. Maintenance actions would be triggered based on a reached degradation threshold.</CommonCoreOntologies:definition>
        <rdfs:label>Degradation assessment module</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_process -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_assessment_process">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/process_precedes"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_threshold_overshoot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A BFO: process performed on a COMSA: physical equipment by a COMSA: predictive maintenance module to assess degradation until this degradation overshoot a specific threshold.</CommonCoreOntologies:definition>
        <rdfs:label>Degradation assessment process</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_threshold_overshoot -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation_threshold_overshoot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Triggering_event"/>
        <CommonCoreOntologies:definition>A COMSA: triggering event subclass related to predictive maintenance strategy. It is prescribed by a degradation assessment module of a predictive maintenance system.</CommonCoreOntologies:definition>
        <rdfs:label>Degradation threshold overshoot</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degraded_condition -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degraded_condition">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition"/>
        <CommonCoreOntologies:definition>A COMSA: Condition sub-class that qualifies a COMSA: physical equipment operating out of its optimal parameters</CommonCoreOntologies:definition>
        <rdfs:label>Degraded condition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Environmental_risk -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Environmental_risk">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_risk"/>
        <CommonCoreOntologies:definition>A CCO: Descriptive information content entity. It describes the environment impact of a failure occurrence.</CommonCoreOntologies:definition>
        <rdfs:label>Environmental risk</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#FMECA_report -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#FMECA_report">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Report"/>
        <rdfs:label>FMECA report</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failed_Condition -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failed_Condition">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition"/>
        <CommonCoreOntologies:definition>A COMSA: Condition sub-class that qualifies the impossibility of a COMSA: Physical equipment to perform its intended function.</CommonCoreOntologies:definition>
        <rdfs:label>Failed condition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Triggering_event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/affects"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/process_preceded_by"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Degradation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A COMSA: triggering event subclass related to corrective maintenance strategy. The imposibility of a physical equipment to perform its intended function triggers a maintenance action.</CommonCoreOntologies:definition>
        <rdfs:label>Failure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_forecast -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_forecast">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Triggering_event"/>
        <CommonCoreOntologies:definition>A COMSA: triggering event subclass related to predictive maintenance strategy. It is prescribed by a failure forecast module of a predictive maintenance system.</CommonCoreOntologies:definition>
        <rdfs:label>Failure forecast</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_forecast_module -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_forecast_module">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Predictive_maintenance_system_module"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000056"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Prognostic_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A COMSA: predictive maintenance system subclass which aims at estimating the remaining useful life of the physical equipment. Maintenance actions would be triggered based on the failure prognostic.</CommonCoreOntologies:definition>
        <rdfs:label>Failure forecast module</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_mode -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_mode">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DescriptiveInformationContentEntity"/>
        <obo:IAO_0000116>EN 13306:2010 defines Failure mode as a manner in which the inability of an item to perform a required function occurs</obo:IAO_0000116>
        <obo:IAO_0000116>ISOÂ 13372:2012 states that a failure is a observable manifestation of a system fault.</obo:IAO_0000116>
        <CommonCoreOntologies:definition>It is a CCO: Descriptive information content entity that describes a failure of a physical equipment and the corresponding fault that can lead to this failure. It is an output of the Failure Modes, Effects and Criticality Analysis (FMECA).</CommonCoreOntologies:definition>
        <rdfs:label>Failure mode</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_probability -->

    <owl:Class rdf:about="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_probability">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ProbabilityMeasurementInformationContentEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_output_of"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Failure_Modes,_Effects_and_Criticality_Analysis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>A CCO: Ratio measurement information content entity that state the probability of a failure occurrence, based statistical information, reliability analysis or design information</CommonCoreOntologies:definition>
        <rdfs:label>Failure probability</rdfs:label>
    </owl:Class>
    


