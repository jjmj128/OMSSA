            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/Artifact -->

    <rdf:Description rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#item"/>
    </rdf:Description>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/NominalStasis -->

    <rdf:Description rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/NominalStasis">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/j.montero-jimenez/ontologies/2020/7/OMSSA#Nominal_operation_process"/>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000002ROMAIN -->

    <owl:Class rdf:about="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000002ROMAIN">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DescriptiveInformationContentEntity"/>
        <obo:IAO_0000115>An IAO:information content entity describing a desired ROM:Maintenance Action</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Maintenance Work Order Specification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000029ROMAIN -->

    <owl:Class rdf:about="http://www.semanticweb.org/hedi/ontologies/2018/11/ROMAIN#OWLClass_ROMAIN00000029ROMAIN">
